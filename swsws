                                            aiming.fov.side = 6
                                            circle.NumSides = 6
                                            aiming.fov.side = 6
                                        elseif aiming.fov.option == "Square" then
                                            aiming.fov.side = 4
                                            circle.NumSides = 4
                                            aiming.fov.side = 4
                                        else
                                        end
                                    end
