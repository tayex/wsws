                                aiming.visualization.forcefieldcol
                            game.Players[targetplr].Character.Head.Color =
                                aiming.visualization.forcefieldcol
                        end
                    end

              

                   

                    if targetplr ~= nil and aiming.visualization.mode == "Ball2" then
